{
  "name": "streamy-player",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/streamy-player/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/streamy-player/browser",
        "index": "apps/streamy-player/src/index.html",
        "main": "apps/streamy-player/src/main.ts",
        "polyfills": [
          "zone.js",
          "node_modules/shaka-player/externs/polyfill.js"
        ],
        "tsConfig": "apps/streamy-player/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/streamy-player/public"
          }
        ],
        "styles": [
          "apps/streamy-player/src/styles.scss",
          "node_modules/shaka-player/dist/controls.css"
        ],
        "scripts": ["node_modules/shaka-player/dist/shaka-player.ui.debug.js"]
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "10mb",
              "maximumError": "20mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "50kb",
              "maximumError": "100kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "continuous": true,
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "proxyConfig": "apps/streamy-player/proxy.conf.json"
      },
      "configurations": {
        "production": {
          "buildTarget": "streamy-player:build:production"
        },
        "development": {
          "buildTarget": "streamy-player:build:development"
        }
      },
      "defaultConfiguration": "development",
      "dependsOn": ["streamy-api:serve"],
      "options": {
        "proxyConfig": "apps/streamy-player/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "streamy-player:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/streamy-player/jest.config.ts",
        "tsConfig": "apps/streamy-player/tsconfig.spec.json"
      }
    },
    "serve-static": {
      "continuous": true,
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "streamy-player:build",
        "port": 4200,
        "spa": true
      }
    },
    "server": {
      "dependsOn": ["build"],
      "executor": "@angular-devkit/build-angular:server",
      "options": {
        "outputPath": "dist/apps/streamy-player/server",
        "main": "apps/streamy-player/src/server.ts",
        "tsConfig": "apps/streamy-player/tsconfig.server.json",
        "inlineStyleLanguage": "scss"
      },
      "configurations": {
        "production": {
          "outputHashing": "media"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "sourceMap": true,
          "extractLicenses": false,
          "vendorChunk": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-ssr": {
      "continuous": true,
      "executor": "@angular-devkit/build-angular:ssr-dev-server",
      "configurations": {
        "development": {
          "browserTarget": "streamy-player:build:development",
          "serverTarget": "streamy-player:server:development"
        },
        "production": {
          "browserTarget": "streamy-player:build:production",
          "serverTarget": "streamy-player:server:production"
        }
      },
      "defaultConfiguration": "development"
    },
    "prerender": {
      "executor": "@angular-devkit/build-angular:prerender",
      "options": {
        "routes": ["/"]
      },
      "configurations": {
        "development": {
          "browserTarget": "streamy-player:build:development",
          "serverTarget": "streamy-player:server:development"
        },
        "production": {
          "browserTarget": "streamy-player:build:production",
          "serverTarget": "streamy-player:server:production"
        }
      },
      "defaultConfiguration": "production"
    }
  }
}
